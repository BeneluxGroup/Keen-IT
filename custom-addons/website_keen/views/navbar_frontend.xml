<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Navbar Frontend -->
    <template id="keen-header" inherit_id="website.layout" name="Keen IT Header">
        <xpath expr="//header" position="replace">
            <header>
                <nav>
                <!-- Nav Desktop -->
                    <div class="keen-navbar-desktop">
                        <div class="keen-navbar-logo">
                            <a href="/">
                                <img class="img-fit" src="/website_keen/static/src/img/base/keen-rgb.svg" alt="Keen IT Logo"/>
                            </a>
                        </div>
                    </div>
                </nav>
            </header>
        </xpath>
    </template>
</odoo>